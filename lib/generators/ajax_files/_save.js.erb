$("ul.errors").html("")
<% if @rt_mod_low.errors.any? %>
  <% @rt_mod_low.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".rt_mod_low-index").html("<%= escape_javascript(render 'index') %>")
  $(".modal").hide();
<% end %>